<div class="card-header bg-primary text-white text-uppercase"><i class="fa fa-list"></i><%= title %></div>
<ul class="list-group category_block">
   <% items.each do |item| %>
     <%= content_tag :li, class: "list-group-item", "onclick": "$('##{filter}').val('#{item.name}');search();"  do %>
       <%= link_to item.name, '#' %>
     <% end %>
  <% end %>
</ul>